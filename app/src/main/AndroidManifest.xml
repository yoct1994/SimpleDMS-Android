<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="simple.dms.myapplication">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_main"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_main_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        android:usesCleartextTraffic="true">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/ad_id_2"/>

        <activity android:name=".ui.login.LoginActivity">
        </activity>
        <activity android:name=".ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.mainpage.MainPageActivity">

        </activity>
        <activity android:name=".ui.goodPoints.GoodActivity">

        </activity>
        <activity android:name=".ui.badPoints.BadActivity">

        </activity>
        <activity android:name=".ui.late.LateActivity">

        </activity>
        <activity android:name=".ui.home.HomeActivity">

        </activity>
    </application>

</manifest>